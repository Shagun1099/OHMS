<div class="container mt-3 ">
        
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger  border border-danger font-weight-bold" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success border border-success font-weight-bold" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>	
